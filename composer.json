{
    "name": "madesimple/twofactor",
    "description": "Library for two factor authentication",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Scopes",
            "email": "peter.scopes@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "MadeSimple\\TwoFactor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "scripts-descriptions": {
        "test": "Run unit and integration tests"
    }
}
